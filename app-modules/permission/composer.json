{
    "name": "misaf/permission",
    "description": "Role-based access control and permission management system using Spatie Laravel Permission",
    "type": "library",
    "version": "v1.0.3",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "permission"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-permission": "^6.20.0"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\Permission\\": "src/",
            "Misaf\\Permission\\Tests\\": "tests/",
            "Misaf\\Permission\\Database\\Factories\\": "database/factories/",
            "Misaf\\Permission\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\Permission\\Providers\\PermissionServiceProvider"
            ]
        }
    }
}
