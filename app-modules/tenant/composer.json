{
    "name": "misaf/tenant",
    "description": "Multi-tenancy management system with isolated data and configuration per tenant",
    "type": "library",
    "version": "v1.0.2",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "tenant"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "staudenmeir/eloquent-has-many-deep": "^1.20.7",
        "staudenmeir/belongs-to-through": "^2.16.4",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/laravel-multitenancy": "^4.0.5"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\Tenant\\": "src/",
            "Misaf\\Tenant\\Tests\\": "tests/",
            "Misaf\\Tenant\\Database\\Factories\\": "database/factories/",
            "Misaf\\Tenant\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\Tenant\\Providers\\TenantServiceProvider"
            ]
        }
    }
}
