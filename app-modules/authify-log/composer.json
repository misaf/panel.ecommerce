{
    "name": "misaf/authify-log",
    "description": "A logging utility for user authentication activities in Laravel.",
    "type": "library",
    "version": "v1.0.1",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "authify",
        "log"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "stijnvanouplines/blade-country-flags": "^1.0.6",
        "spatie/laravel-package-tools": "^1.92.4"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\AuthifyLog\\": "src/",
            "Misaf\\AuthifyLog\\Tests\\": "tests/",
            "Misaf\\AuthifyLog\\Database\\Factories\\": "database/factories/",
            "Misaf\\AuthifyLog\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\AuthifyLog\\Providers\\AuthifyLogServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../app-modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
