{
    "name": "misaf/language",
    "description": "Multi-language support and translation management for internationalization",
    "type": "library",
    "version": "v1.0.4",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "language"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/eloquent-sortable": "^4.5.0",
        "spatie/laravel-translation-loader": "^2.8.2"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\Language\\": "src/",
            "Misaf\\Language\\Tests\\": "tests/",
            "Misaf\\Language\\Database\\Factories\\": "database/factories/",
            "Misaf\\Language\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\Language\\Providers\\LanguageServiceProvider",
                "Misaf\\Language\\Providers\\LanguageDeferredServiceProvider"
            ]
        }
    }
}
