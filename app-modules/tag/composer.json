{
    "name": "misaf/tag",
    "description": "Tagging system for categorizing and organizing content across the application",
    "type": "library",
    "version": "v1.0.4",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "tag"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/eloquent-sortable": "^4.5.0"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\Tag\\": "src/",
            "Misaf\\Tag\\Tests\\": "tests/",
            "Misaf\\Tag\\Database\\Factories\\": "database/factories/",
            "Misaf\\Tag\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\Tag\\Providers\\TagServiceProvider"
            ]
        }
    }
}
