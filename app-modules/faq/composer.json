    "type": "library",
    "version": "v1.0.3",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "faq"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-translatable": "^6.11.4",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/eloquent-sortable": "^4.5.0"
    },
    "autoload": {
