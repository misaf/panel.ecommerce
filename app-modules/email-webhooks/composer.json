{
    "name": "misaf/email-webhooks",
    "description": "Core package for handling email webhook events in Laravel applications.",
    "type": "library",
    "version": "v1.0.1",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "email",
        "webhook",
        "event",
        "integration"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "orchestra/testbench": "^9.0"
    },
    "scripts": {
        "test": "pest"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\EmailWebhooks\\": "src/",
            "Misaf\\EmailWebhooks\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\EmailWebhooks\\Providers\\EmailWebhooksServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
