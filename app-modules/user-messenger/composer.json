{
    "name": "misaf/user-messenger",
    "description": "Internal messaging system for user-to-user communication",
    "type": "library",
    "version": "v1.0.1",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "user",
        "messenger"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-activitylog": "^4.10.2",
        "misaf/user": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\UserMessenger\\": "src/",
            "Misaf\\UserMessenger\\Tests\\": "tests/",
            "Misaf\\UserMessenger\\Database\\Factories\\": "database/factories/",
            "Misaf\\UserMessenger\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\UserMessenger\\Providers\\UserMessengerServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../app-modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
