{
    "name": "misaf/transaction",
    "description": "Financial transaction management system with multi-gateway support, status tracking and comprehensive metadata handling",
    "type": "library",
    "version": "v1.0.3",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "transaction",
        "payment",
        "gateway",
        "finance"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-sluggable": "^3.7.5",
        "spatie/eloquent-sortable": "^4.5.0"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\Transaction\\": "src/",
            "Misaf\\Transaction\\Tests\\": "tests/",
            "Misaf\\Transaction\\Database\\Factories\\": "database/factories/",
            "Misaf\\Transaction\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\Transaction\\Providers\\TransactionServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
