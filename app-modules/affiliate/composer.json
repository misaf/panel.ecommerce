{
    "name": "misaf/affiliate",
    "description": "Affiliate and referral management system with commission tracking and user referral capabilities",
    "type": "library",
    "version": "v1.0.1",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "affiliate",
        "referral",
        "commission"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-activitylog": "^4.10.2",
        "spatie/laravel-sluggable": "^3.7.5"
    },
    "autoload": {
        "psr-4": {
            "Misaf\\Affiliate\\": "src/",
            "Misaf\\Affiliate\\Tests\\": "tests/",
            "Misaf\\Affiliate\\Database\\Factories\\": "database/factories/",
            "Misaf\\Affiliate\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Misaf\\Affiliate\\Providers\\AffiliateServiceProvider"
            ]
        }
    }
}
