    "type": "library",
    "version": "v1.0.1",
    "license": "proprietary",
    "keywords": [
        "laravel",
        "user",
        "level"
    ],
    "authors": [
        {
            "name": "Misaf",
            "email": "misaf.1990@gmail.com"
        }
    ],
    "require": {
